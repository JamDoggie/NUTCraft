{
  "schemaVersion": 1,
  "id": "nutcraft",
  "version": "0.1",

  "name": "NUT Craft",
  "description": "Adds a gui to minecraft that lets you use the Squirrel scripting API",
  "authors": [
    "Squidski",
    "JamDoggie",
    "KaleMonkey"
  ],
  "contact": {
    "homepage": "https://github.com/JamDoggie/NUTCraft",
    "sources": "https://github.com/JamDoggie/NUTCraft"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.nutcraft.main.Main"
    ],
    "client": [
      "net.nutcraft.main.ClientMain"
    ]
  },
  "mixins": [
    "nutcraft.mixins.json"
  ],

  "requires": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
